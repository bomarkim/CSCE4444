
@{
    ViewData["Title"] = "Sign In";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Sign In</h2>

<form>
    <div class="form-row">
        <div class="form-group">
            <label for="Email">Email</label>
            <input id="Email" ng-model="form.email" class="form-control" type="text" />
        </div>
    </div>
    <div class="form-row">
        <div class="form-group">
            <label for="password">Password</label>
            <input id="password" ng-model="form.password" class="form-control" type="password" />
        </div>
    </div>
    <button type="submit" class="btn-lg btn-red darken-3">Sign In</button>
    <button type="reset" class="btn-lg btn-grey">Clear</button>
</form>

